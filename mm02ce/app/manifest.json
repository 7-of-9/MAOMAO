{
  "name": "mm02ce",
  "version": "0.2",
  "description": "Hope++!",
  "background": {
    "page": "bg.html"
  },
  "options_page": "pages/options.html",
  "icons": { "128": "img/ps_sirius_dog_blue.png" },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "css": [ ],
      "js": [ "./build/content.js" ]
    }
  ],
  "browser_action": {
    "default_title": "FS browser action",
    "default_icon": "img/icon.png"
  },
  "manifest_version": 2
}