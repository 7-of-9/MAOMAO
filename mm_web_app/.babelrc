{
  "presets": [
      "next/babel",
      "env",
      "stage-0"
    ],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "plugins": ["transform-inline-environment-variables"]
    }
  },
  "plugins": [
    "transform-decorators-legacy",
    "inline-react-svg",
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "scss$"
        ]
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": true } ]
  ],
  "ignore": []
}
