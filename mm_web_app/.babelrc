{
  "presets": [
      "next/babel"
    ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "scss$"
        ]
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
  ],
  "ignore": []
}
